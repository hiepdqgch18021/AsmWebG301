{% extends 'base.html.twig' %}

{% block title %}Semester{% endblock %}

{% block body %}
    <h1>Semester</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ semester.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ semester.name }}</td>
            </tr>
            <tr>
                <th>TimeStart</th>
                <td>{{ semester.timeStart ? semester.timeStart|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>TimeEnd</th>
                <td>{{ semester.timeEnd ? semester.timeEnd|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Tuition</th>
                <td>{{ semester.tuition }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('crud_semester_index') }}">back to list</a>

    <a href="{{ path('crud_semester_edit', {'id': semester.id}) }}">edit</a>

    {{ include('crud_semester/_delete_form.html.twig') }}
{% endblock %}
